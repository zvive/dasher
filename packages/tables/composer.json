{
    "name": "dasher/tables",
    "description": "Effortlessly build TALL-powered tables.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-dasher/dasher",
    "support": {
        "issues": "https://github.com/laravel-dasher/dasher/issues",
        "source": "https://github.com/laravel-dasher/dasher"
    },
    "require": {
        "php": "^8.0",
        "akaunting/laravel-money": "^1.2|^2.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "dasher/forms": "^1.1",
        "illuminate/console": "^8.6|^9.0",
        "illuminate/contracts": "^8.6|^9.0",
        "illuminate/database": "^8.6|^9.0",
        "illuminate/filesystem": "^8.6|^9.0",
        "illuminate/support": "^8.6|^9.0",
        "illuminate/view": "^8.6|^9.0",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "zvive/fixer": "^v9.0.6"
    },
    "autoload": {
        "psr-4": {
            "Dasher\\Tables\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasher\\Tables\\TablesServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../forms"
        }
    ]
}
