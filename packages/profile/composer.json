{
    "name": "dasher/profile",
    "description": "A simple profile page for Dasher.",
    "homepage": "https://github.com/zvive/dasher",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Curl",
            "email": "patrickwcurl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "dasher/dasher": "^1.0.8",
        "illuminate/contracts": "^8.6|^9.0",
        "illuminate/support": "^8.6|^9.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.0",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.17",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.29",
        "zvive/fixer": "^v9.0.6"
    },
    "autoload": {
        "psr-4": {
            "Dasher\\Profile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dasher\\Profile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasher\\Profile\\ProfileServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        },
        {
            "type": "path",
            "url": "../forms"
        },
        {
            "type": "path",
            "url": "../tables"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
