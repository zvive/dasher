{
    "name": "dasher/forms",
    "description": "Effortlessly build TALL-powered forms.",
    "license": "MIT",
    "homepage": "https://github.com/zvive/dasher",
    "support": {
        "issues": "https://github.com/zvive/dasher/issues",
        "source": "https://github.com/zvive/dasher"
    },
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "danharrin/date-format-converter": "^0.2",
        "illuminate/console": "^8.6|^9.0",
        "illuminate/contracts": "^8.6|^9.0",
        "illuminate/database": "^8.6|^9.0",
        "illuminate/filesystem": "^8.6|^9.0",
        "illuminate/support": "^8.6|^9.0",
        "illuminate/validation": "^8.6|^9.0",
        "illuminate/view": "^8.6|^9.0",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "zvive/fixer": "^v9.0.6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Dasher\\Forms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasher\\Forms\\FormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
