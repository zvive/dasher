{
    "name": "dasher/dasher",
    "description": "Effortlessly build TALL-powered admin panels.",
    "license": "MIT",
    "homepage": "https://github.com/zvive/dasher",
    "support": {
        "issues": "https://github.com/zvive/dasher/issues",
        "source": "https://github.com/zvive/dasher"
    },
    "require": {
        "php": "^8.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "akaunting/laravel-money": "^1.2|^2.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "canvural/phpstan-blade-rule": "^0.1.0",
        "danharrin/livewire-rate-limiting": "^0.3|^1.0",
        "doctrine/dbal": "^3.2",
        "league/flysystem-aws-s3-v3": "^1.0|^2.0|^3.0",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.17",
        "pestphp/pest-plugin-laravel": "^1.0",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3",
        "phpstan/extension-installer": "^1.1",
        "spatie/laravel-medialibrary": "^9.11|^10.0",
        "spatie/laravel-package-tools": "^1.9",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-tags": "^4.2",
        "symplify/monorepo-builder": "^10.0",
        "zvive/fixer": "^v9.0.6"
    },

    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Dasher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasher\\DasherServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
